def greaternumber(a,b,c):
  if a>b and a>c:
    print("a is greater")
  elif(b>a and b>c):
    print("b is greater")
  elif(c>a and c>b):
    print("c is greater")
  else:
    print("All Number Are Greater")
a=int(input("Enter First Number:"))
b=int(input("Enter Second Number:"))
c=int(input("Enter Third Number:"))
greaternumber(a,b,c)
